pipeline {
    agent any
    stages {
        stage('Cleanup') {
            steps {
                cleanWs()
            }
        }

        stage('Clone Git Repo') {
            steps {
                checkout scm
            }
        }

        stage('built and run') {
            steps {
                sh 'docker build -t Image1 .'
                sh 'docker run -d -p 8081:80 Image1'
            }
        }

       
    }
}
